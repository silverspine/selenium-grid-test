  "main": "index.js",
  "scripts": {
    "nightwatch-test": "nightwatch --env chrome_local",
    "webdriver-test": "wdio wdio.conf.js",
    "jest-test": "jest --config jest.config.js",
    "mocha-test": "mocha mocha/google.spec.js",
